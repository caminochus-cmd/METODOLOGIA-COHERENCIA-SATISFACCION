{
  "version": 2,
  "name": "c3-consultoria-estrategica",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/cuestionarios/(.*)",
      "dest": "/cuestionarios/$1"
    }
  ]
}
